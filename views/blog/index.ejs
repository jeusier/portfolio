<div class="jumbotron">
    <h1>Welcome to console.blog(Mind of jCurray)</h1>
        <p class="lead">Venture into the dark abyss known as the mind of jCurray</p>
</div> <!-- end of div jumbotron -->

<div class="row main">
    <div class="col-lg-12">
        <% blogs.forEach(function(blog){ %>
        <div class="section-block">
            <a href="/blog/<%= blog.id %>">
                <h2><%= blog.title %></h4>
            </a>
            <p><%= blog.content %></p>
        </div>
        <% }) %>
    </div> <!-- end of div col-lg-12 -->
</div> <!-- end of div row main -->